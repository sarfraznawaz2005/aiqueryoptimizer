{
    "name": "sarfraznawaz2005/aiqueryoptimizer",
    "description": "A Laravel package to analyze and optimize SQL queries using AI.",
    "type": "laravel-package",
    "license": "MIT",
    "keywords": ["laravel", "sql", "ai", "query", "optimizer", "performance", "database", "gemini", "openai"],
    "authors": [
        {
            "name": "Sarfraz Ahmed",
            "email": "sarfraznawaz2005@gmail.com",
            "homepage": "https://github.com/sarfraznawaz2005/aiqueryoptimizer/graphs/contributors"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "inspector-apm/neuron-ai": "^1.0",
        "erusev/parsedown": "^1.7",
        "erusev/parsedown-extra": "^0.8.1",
        "tempest/highlight": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AIQueryOptimizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AIQueryOptimizer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AIQueryOptimizer\\AIQueryOptimizerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^10.6",
        "phpstan/phpstan": "^2.1"
    }
}
